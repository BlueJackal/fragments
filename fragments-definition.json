{
  "family": "fragments-task",
  "containerDefinitions": [
    {
      "name": "fragments-container",
      "image": "simonizer/fragments:latest",
      "essential": true,
      "portMappings": [
        {
          "name": "fragments-container-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "PORT",
          "value": "80"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "LOG_LEVEL",
          "value": "info"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "AWS_S3_BUCKET_NAME",
          "value": "csimon-fragments"
        },
        {
          "name": "AWS_COGNITO_POOL_ID",
          "value": "us-east-1_59v1tcHFd"
        },
        {
          "name": "AWS_COGNITO_CLIENT_ID",
          "value": "4vko4s45m9795767njrj9b62le"
        },
        {
          "name": "API_URL",
          "value": "https://3.86.251.222"
        },
        {
          "name": "HTPASSWD_FILE",
          "value": "tests/.htpasswd"
        },
        {
          "name": "AWS_DYNAMODB_TABLE_NAME",
          "value": "fragments"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/fragments-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m"
        }
      }
    }
  ],
  "executionRoleArn": "arn:aws:iam::184226208422:role/LabRole",
  "taskRoleArn": "arn:aws:iam::184226208422:role/LabRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
